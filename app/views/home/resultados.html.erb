
  <div class="row">



 <!-- Nav Sidebar -->
        <!-- Main Feed -->
    <!-- This has been source ordered to come first in the markup (and on small devices) but to be to the right of the nav on larger screens -->
    <div class="large-6 columns push-3">

      <!-- Feed Entry -->

      <% @productos.each do |producto| %>
      <div class="row">
        <div class="small-3 large-2 columns "><img src="<%= producto.img %>" /></div>
        <div class="small-9 large-10 columns">
          <p><strong><%= producto.nombre %></strong> $ <%= producto.precio %></p>
          <ul class="inline-list">
            <li><a href="<%= url_for producto %>">ver mas...</a></li>
          </ul>

        </div>
      </div>


  
      <!-- End Feed Entry -->

      <hr />

<%end%>

    </div>

    <!-- This is source ordered to be pulled to the left on larger screens -->
    <div class="large-3 columns pull-6">

    </div>
 
 
    <!-- Right Sidebar -->
    <!-- On small devices this column is hidden -->
    <aside class="large-3 columns hide-for-small">
      <p><img src="http://placehold.it/300x440&text=[ad]" /></p>
      <p><img src="http://placehold.it/300x440&text=[ad]" /></p>
    </aside>

  </div>